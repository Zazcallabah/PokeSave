<UserControl x:Class="PokeEdit.MovePropertyPanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:PokeEdit="clr-namespace:PokeEdit">
	<Expander Name="Header">
		<StackPanel>
			<PokeEdit:PropertyPanel x:Name="Move" DataContext="{Binding}"/>
			<PokeEdit:PropertyPanel x:Name="PP" DataContext="{Binding}"/>
			<PokeEdit:PropertyPanel x:Name="PPBonus" DataContext="{Binding}"/>
		</StackPanel>
	</Expander>
</UserControl>
